.form-search
  .span4= label_tag t(:search_paciente)
  .span3= select_tag "paciente_id",  options_from_collection_for_select(Paciente.all, 'id', 'full_name_search'), :class => 'chzn-select'
  %button.btn.span1{:onClick => "edit_paciente()"} Buscar 

:javascript
  function edit_paciente(){
    var paciente_id = $("#search_client_id").val();
    var url = '/pacientes/'+paciente_id+'/edit';
    javscript:window.location.href = url;
  }
  
